# UI Application Environment Variables
# Copy this file to .env.local and adjust values

# Core API Configuration
NEXT_PUBLIC_CORE_API_URL=http://localhost:8000
NEXT_PUBLIC_CORE_API_KEY=dev-api-key-change-in-production

# UI Configuration
NEXT_PUBLIC_APP_NAME=Jarvis Dashboard
NEXT_PUBLIC_APP_VERSION=0.1.0

# Development
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# API Endpoints
NEXT_PUBLIC_METRICS_ENDPOINT=/metrics
NEXT_PUBLIC_LOGS_ENDPOINT=/logs/stream
NEXT_PUBLIC_SELFTEST_ENDPOINT=/selftest

# Feature Flags
NEXT_PUBLIC_ENABLE_SSE=true
NEXT_PUBLIC_ENABLE_METRICS=true
NEXT_PUBLIC_ENABLE_SELFTEST=true

# Monitoring
NEXT_PUBLIC_SENTRY_DSN=
NEXT_PUBLIC_ANALYTICS_ID=
